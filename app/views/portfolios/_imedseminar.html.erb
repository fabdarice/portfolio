<script type="text/javascript">	
	$(function() {	
		$(".project_text_description").delay("slow").animate({ left: '-25px', opacity: '1'}, 1000);
		$(".project_slideshow").delay("slow").animate({ right: '-25px', opacity: '1'}, 1000);
		$('#slider').nivoSlider();	
	});
</script>
<div class="portfolio_project">
	<div class="project_text_description">
		<span class="project_title"><span class="orange_underline">I</span>MED <span class="orange_underline">S</span>eminar</span>
		<br><br>
		<p  class="project_description">
			From Eric Schmidt (Google), Henry Samueli (Broadcom) to Jeannie Buss (Los Angeles Lakers), imedseminar is a weekly gathering site for leaders in science, medicine, industry, policy, communication, and creative thinking.
		</p>
		<p class="project_link">Link : <a href="http://imedseminar.com" target="__blank">http://imedseminar.com</a></p>
		<div class="project_skills">
			<ul>
				<li>Ruby on Rails</li>
				<li>JQuery</li>
				<li>MySQL</li>
			</ul>
		</div>

	</div>
	<div class="project_slideshow">
      <div class="slider-wrapper theme-default">    
			<div id="slider" class="nivoSlider">
				<%= image_tag "projects/imedseminar_01.png" %>
			   <%= image_tag "projects/imedseminar_02.png" %>
			   <%= image_tag "projects/imedseminar_03.png" %>
			   <%= image_tag "projects/imedseminar_04.png" %>
			   <%= image_tag "projects/imedseminar_05.png" %>
			   <%= image_tag "projects/imedseminar_06.png" %>
			</div>		
		</div>	
	</div>
</div>