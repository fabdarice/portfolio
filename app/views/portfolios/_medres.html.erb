<script type="text/javascript">	
	$(function() {	
		$(".project_text_description").delay("slow").animate({ left: '-25px', opacity: '1'}, 800);
		$(".project_slideshow").delay("slow").animate({ right: '-25px', opacity: '1'}, 800);
		$('#slider').nivoSlider();	
	});
</script>
<div class="portfolio_project">
	<div class="project_text_description">
		<span class="project_title"><span class="orange_underline">M</span>edRes</span>
		<br><br>
		<p  class="project_description">
			MedRes is the web support for UCLA Internal Medicine residents.	
		</p>		
		<p class="project_link">Link : <a href="http://medres.med.ucla.edu" target="__blank">http://medres.med.ucla.edu</a></p>
		<div class="project_skills">
			<ul>
				<li>Joomla</li>
				<li>PHP</li>
				<li>MySQL</li>				
			</ul>
		</div>

	</div>
	<div class="project_slideshow">
      <div class="slider-wrapper theme-default">    
			<div id="slider" class="nivoSlider">
				<%= image_tag "projects/medres_01.png" %>
			   <%= image_tag "projects/medres_02.png" %>			   
			</div>		
		</div>	
	</div>
</div>